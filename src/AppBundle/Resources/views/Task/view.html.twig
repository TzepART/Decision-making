{% extends 'base.html.twig' %}

{% block body %}
    {{ task.name }}
    <ul>
    {% for criteria in task.criteria %}
        <li>{{ criteria.name }}</li>
    {% endfor %}
    </ul>

    <ul>
        {% for variant in task.variants %}
            <li>{{ variant.name }}</li>
        {% endfor %}
    </ul>

{% endblock %}

